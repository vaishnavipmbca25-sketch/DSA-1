#include <stdio.h>

int main() {
    int a[100], n, i;
    int larg, small;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    larg = a[0];
    small = a[0];

    for(i = 1; i < n; i++) {
        if(a[i] > larg) {
            larg = a[i];
        }

        if(a[i] < small) {
            small = a[i];
        }
    }

    printf("Largest = %d\n", larg);
    printf("Smallest = %d\n", small);

    return 0;
}